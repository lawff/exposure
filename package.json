{
  "name": "@lawlietff/exposure",
  "version": "0.2.1",
  "packageManager": "pnpm@6.32.3",
  "description": "handle dom exposure",
  "keywords": [
    "ad",
    "dom",
    "exposure"
  ],
  "homepage": "https://github.com/evillawliet/ad-exposure#readme",
  "bugs": {
    "url": "https://github.com/evillawliet/ad-exposure/issues"
  },
  "license": "MIT",
  "author": "lawliet",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evillawliet/ad-exposure.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "serve": "nr build && vite demo",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "intersection-observer": "^0.12.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.9",
    "@antfu/ni": "^0.13.2",
    "@babel/types": "^7.17.0",
    "@types/node": "^17.0.23",
    "bumpp": "^7.1.1",
    "eslint": "^8.11.0",
    "esno": "^0.14.1",
    "pnpm": "^6.32.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2",
    "unbuild": "^0.7.0",
    "vite": "^2.8.6",
    "vitest": "^0.7.10"
  }
}
